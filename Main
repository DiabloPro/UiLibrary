local UiLibrary = {}
UiLibrary.__index = UiLibrary

function UiLibrary.init(name)
    local folder = game:GetObjects("9820680667")[1]
	local screenGui = folder.ScreenGui:Clone()
	local menu = screenGui.Menu
	menu.TopBar.Title = name
	Screen.Parent = CoreGui

	menu.TopBar.Destroy.MouseButton1Click:Connect(function()
		screenGui:Destroy()
	end)

	return setmetatable({
		folder = folder,
		screenGui = screenGui,
		windows = {},
        colorable = colorable
	}, UiLibrary)
end
return UiLibrary
